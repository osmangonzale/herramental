<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Maquina</servlet-name>
        <servlet-class>Servlets.Maquina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Herramental</servlet-name>
        <servlet-class>Servlets.Herramental</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Movimiento</servlet-name>
        <servlet-class>Servlets.Movimiento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tipo_maquina</servlet-name>
        <servlet-class>Servlets.Tipo_maquina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Programacion</servlet-name>
        <servlet-class>Servlets.Programacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Defecto</servlet-name>
        <servlet-class>Servlets.Defecto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Clasificacion_defecto</servlet-name>
        <servlet-class>Servlets.Clasificacion_defecto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Familia_producto</servlet-name>
        <servlet-class>Servlets.Familia_producto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consulta_catologo</servlet-name>
        <servlet-class>Servlets.Consulta_catologo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Informe</servlet-name>
        <servlet-class>Servlets.Informe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bitacora</servlet-name>
        <servlet-class>Servlets.Bitacora</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Nota</servlet-name>
        <servlet-class>Servlets.Nota</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ficha_tecnica</servlet-name>
        <servlet-class>Servlets.Ficha_tecnica</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Maquina</servlet-name>
        <url-pattern>/Maquina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Herramental</servlet-name>
        <url-pattern>/Herramental</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Movimiento</servlet-name>
        <url-pattern>/Movimiento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tipo_maquina</servlet-name>
        <url-pattern>/Tipo_maquina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Programacion</servlet-name>
        <url-pattern>/Programacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Defecto</servlet-name>
        <url-pattern>/Defecto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Clasificacion_defecto</servlet-name>
        <url-pattern>/Clasificacion_defecto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Familia_producto</servlet-name>
        <url-pattern>/Familia_producto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consulta_catologo</servlet-name>
        <url-pattern>/Consulta_catologo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Informe</servlet-name>
        <url-pattern>/Informe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bitacora</servlet-name>
        <url-pattern>/Bitacora</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Nota</servlet-name>
        <url-pattern>/Nota</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ficha_tecnica</servlet-name>
        <url-pattern>/Ficha_tecnica</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            120
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>Connector</servlet-name>
        <servlet-class>com.fredck.FCKeditor.connector.ConnectorServlet</servlet-class>
        <init-param>
            <param-name>baseDir</param-name>
            <param-value>/UserFiles/</param-value>
        </init-param>
        <init-param>
            <param-name>debug</param-name>
            <param-value>true</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>SimpleUploader</servlet-name>
        <servlet-class>com.fredck.FCKeditor.uploader.SimpleUploaderServlet</servlet-class>
        <init-param>
            <param-name>baseDir</param-name>
            <param-value>/UserFiles/</param-value>
        </init-param>
        <init-param>
            <param-name>debug</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>enabled</param-name>
            <param-value>false</param-value>
        </init-param>
        <init-param>
            <param-name>AllowedExtensionsFile</param-name>
            <param-value/>
        </init-param>
        <init-param>
            <param-name>DeniedExtensionsFile</param-name>
            <param-value>php|php3|php5|phtml|asp|aspx|ascx|jsp|cfm|cfc|pl|bat|exe|dll|reg|cgi</param-value>
        </init-param>
        <init-param>
            <param-name>AllowedExtensionsImage</param-name>
            <param-value>jpg|gif|jpeg|png|bmp</param-value>
        </init-param>
        <init-param>
            <param-name>DeniedExtensionsImage</param-name>
            <param-value/>
        </init-param>
        <init-param>
            <param-name>AllowedExtensionsFlash</param-name>
            <param-value>swf|fla</param-value>
        </init-param>
        <init-param>
            <param-name>DeniedExtensionsFlash</param-name>
            <param-value/>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Connector</servlet-name>
        <url-pattern>/tinyfck/filemanager/connectors/jsp/connector</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SimpleUploader</servlet-name>
        <url-pattern>/tinyfck/filemanager/upload/simpleuploader</url-pattern>
    </servlet-mapping>
</web-app>
